  {
    "name": "Discord Link Sender",
    "description" : "Base Level Extension",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
      "128": "Discord-Logo-White.png"
    },
    "browser_action": {
      "default_popup": "hello.html",
      "default_icon": "Discord-Logo-White.png"
    },

    "permissions": [
        "contextMenus"
    ],

    "background": {
        "scripts": ["eventPage.js"],
        "persistent": false
    },


    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+F",
          "mac": "MacCtrl+Shift+F"
        },
        "description": "Opens hello.html"
      }
    }
  
  }